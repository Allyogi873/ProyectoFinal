@model ProyectoInde.Models.ControlGeneracionDiaria

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}
<style>
    /*Diseño boton*/
    .boton_personalizado {
        text-decoration: none;
        /*padding: 10px;*/
        font-weight: 600;
        font-size: 20px;
        color: #ffffff;
        background-color: #1883ba;
        border-radius: 6px;
        border: 2px solid #0016b0;
    }

        .boton_personalizado:hover {
            color: #1883ba;
            background-color: transparent;
        }

    .boton_alternativo {
        background-color: #00c69f;
        color: #ffffff !important;
        border: 2px solid #006400;
        border-radius: 7px;
    }

        .boton_alternativo:hover {
            background-color: transparent !important;
            color: #00c69f !important;
            text-decoration: none;
        }
    /*Diseño Div cont*/
    .contpadre {
        background: transparent;
        height: 1000px;
        /*IMPORTANTE*/
        display: flex;
        justify-content: center;
        align-items: center;
    }

    .conthijo {
        background: transparent;
        width: 600px;
    }
</style>

<body id="top">
    <div class="encabezado">
        <b>INGRESAR CONTROL GENERACIONES DIARIAS</b>
    </div>
    <section id='services' class="s-services">
        <div class="contpadre">
            <div class="conthijo">

                <div class="row">
                    <div class="col-md-12">
                        <form asp-action="Create">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <div class="form-group">
                                <label asp-for="_230kVA" class="control-label"></label>
                                <input asp-for="_230kVA" class="form-control" />
                                <span asp-validation-for="_230kVA" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="CampoV" class="control-label"></label>
                                <input asp-for="CampoV" class="form-control" />
                                <span asp-validation-for="CampoV" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="CampoA" class="control-label"></label>
                                <input asp-for="CampoA" class="form-control" />
                                <span asp-validation-for="CampoA" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="_138kvA" class="control-label"></label>
                                <input asp-for="_138kvA" class="form-control" />
                                <span asp-validation-for="_138kvA" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="_138kvKv" class="control-label"></label>
                                <input asp-for="_138kvKv" class="form-control" />
                                <span asp-validation-for="_138kvKv" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="PAparenteMw" class="control-label"></label>
                                <input asp-for="PAparenteMw" class="form-control" />
                                <span asp-validation-for="PAparenteMw" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="PAparenteMvar" class="control-label"></label>
                                <input asp-for="PAparenteMvar" class="form-control" />
                                <span asp-validation-for="PAparenteMvar" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="FecIngreso" class="control-label"></label>
                                <input asp-for="FecIngreso" class="form-control" />
                                <span asp-validation-for="FecIngreso" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="CodHora" class="control-label"></label>
                                <select asp-for="CodHora" class="form-control" asp-items="ViewBag.CodHora"></select>
                            </div>
                            <div class="form-group">
                                <label asp-for="CodUnidadGeneradora" class="control-label"></label>
                                <select asp-for="CodUnidadGeneradora" class="form-control" asp-items="ViewBag.CodUnidadGeneradora"></select>
                            </div>
                            <div style='text-align:center' class="form-group">
                                <input type="submit" value="Crear" class="btn boton_alternativo"  /> <a class="btn boton_alternativo" asp-action="Index">Regresar a la Lista</a>
                            </div>
                        </form>
                    </div>
                </div>

               


            </div>
        </div>

    </section>
</body>
